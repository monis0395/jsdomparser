{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "**/*.test.ts"
    ]
  },
  "jsRules": {},
  "rules": {
    "no-any": true,
    "forin": false,
    "no-for-in": false,
    "interface-name": false,
    "ordered-imports": false,
    "member-ordering": true,
    "interface-over-type-literal": false,
    "max-line-length": [
      true,
      150
    ]
  },
  "rulesDirectory": []
}
